<html lang="en" ng-app="ZamkaAdmin">
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/0.10.0/angular-material.min.css">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css">
  <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/animate.css/3.2.6/animate.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.0/sweetalert.min.css">
    <link rel="stylesheet" type="text/css" href="/css/md-date-time.css">
  <link rel="stylesheet" type="text/css" href="/css/app.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/locale/es.js"></script>
</head>
<body ng-controller="AppCtrl">
<md-toolbar>
    <div class="md-toolbar-tools">
        <a href="/App"><img class="heightFull" src="/img/logow.png"></a>
        <span flex></span>

        <md-button ng-hide="usuario" class="md-icon-button" aria-label="More">
            <md-tooltip md-direction="down">
                Ingresar
            </md-tooltip>
            <i class="fa fa-sign-in"></i>
        </md-button>


        <md-button ng-show="usuario" class="md-icon-button" aria-label="More">
            <md-tooltip md-direction="down">
                Mi Perfil
            </md-tooltip>
            <i class="fa fa-users"></i>
        </md-button>
        <md-button ng-show="usuario" class="md-icon-button" aria-label="More">
            <md-tooltip md-direction="down">
                Mi Cuenta
            </md-tooltip>
            <i class="fa fa-inbox"></i>
        </md-button>
        <md-button ng-show="usuario" class="md-icon-button" aria-label="Salir" ng-click="logout()">
            <md-tooltip md-direction="down">
                Salir
            </md-tooltip>
            <i class="fa fa-sign-out"></i>
        </md-button>
    </div>
</md-toolbar>

<div class="loadingBack" layout="row" layout-align="center center" ng-show="cargando">
    <md-progress-circular class="md-warn" md-mode="indeterminate"></md-progress-circular>
</div>

<div class="contenedor" layout="row" layout-align="center start" flex ng-view>
</div>


<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-animate.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-aria.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-route.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angular_material/0.10.0/angular-material.min.js"></script>
<script src="http://cdnjs.cloudflare.com/ajax/libs/angular-i18n/1.2.15/angular-locale_es-es.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.0/sweetalert.min.js"></script>
<script src="/js/md-date-time.js"></script>




<script src="/js/app.js"></script>
</body>
</html>

